<Window x:Class="Dependencies.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Dependency Manager" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button Name="btnOpen" Grid.Row="0" Grid.Column="0" Content="Open File..." Margin="10" Click="btnOpen_Click"/>
        <ListBox Grid.Row="1" Grid.Column="0" Margin="10" ItemsSource="{Binding OutputList}"/>
    </Grid>
</Window>
